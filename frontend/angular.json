{
  "$schema": "https://json.schemastore.org/angular-cli",
  "version": 1,
  "projects": {
    "encrypt-frontend": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "tsConfig": "tsconfig.app.json",
            "outputPath": "dist/encrypt-frontend",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 4200,
            "buildTarget": "encrypt-frontend:build"
          }
        }
      }
    }
  }
}

